<% group_file_list.each do |file_name| %>require_relative '<%= file_name %>'
<% end %>
module USCore
  class <%= class_name %> < Inferno::TestSuite
    title '<%= title %>'
    # description ''

    id :<%= suite_id %>

    input :url, default: 'https://inferno.healthit.gov/reference-server/r4'
    input :bearer_token, default: 'SAMPLE_TOKEN'

    fhir_client do
      url :url
      bearer_token :bearer_token
    end
<% group_id_list.each do |id| %>
    group from: :<%= id %><% end %>
  end
end
